{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "description": "A bar graph showing the scores of the top 5 students. This shows an example of the window transform, for how the top K (5) can be filtered, and also how a rank can be computed for each student.",
  "data": {
    "url": "suicide_g.csv"
  },
  "title": {
    "text": "Top 10 countries of suicide rate",
    "titleAnchor": "start"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "2015",
      "type": "quantitative",
      "axis": {
        "title": "Suicide rate (per 100k population)"
      }
    },
    "y": {
      "field": "generation",
      "type": "nominal",
      "sort": {
        "field": "2015",
        "op": "average",
        "order": "descending"
      },
      "axis": {
        "title": "Generation"
      }
    }
  }
}

